# WAHA Configuration
# Start WAHA with: docker run -p 3000:3000 --env-file waha-config.env devlikeapro/waha

# API Configuration
API_ENABLED=true
# WAHA_API_KEY=berhasil123

# Engine Configuration
ENGINE=nowjs
STARTUP_OPTIONS=--no-sandbox --disable-setuid-sandbox

# Webhook Configuration
WEBHOOK_URL=http://localhost:3001/webhook
WEBHOOK_EVENTS=message,ack

# Session Configuration
SESSIONS=default
DEFAULT_SESSION=default

# Security
CORS_ENABLED=true
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info